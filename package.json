{
	"name": "express-microservices",
	"version": "1.0.0",
	"description": "Express microservices tecnical test from Deall Jobs (YC W22).",
	"scripts": {
		"install:shared": "npm install --log-level verbose --prefix shared",
		"install:users": "npm install --log-level verbose --prefix services/users",
		"install:roles": "npm install --log-level verbose --prefix services/roles",
		"build:shared": "npm run build --prefix shared",
		"build:users": "npm run build --prefix services/users",
		"build:roles": "npm run build --prefix services/roles",
		"start:users": "npm run start --prefix services/users",
		"start:roles": "npm run start --prefix services/roles",
		"dev:users": "npm run dev --prefix services/users",
		"dev:roles": "npm run dev --prefix services/roles",
		"install": "npm-run-all -p install:*",
		"build": "npm-run-all -p build:*"
	},
	"author": {
		"name": "Restu Wahyu Saputra",
		"email": "restuwahyu13@gmail.com",
		"url": "https://github.com/restuwahyu13"
